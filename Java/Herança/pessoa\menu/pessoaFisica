package modelo;

public class PessoaFisica extends Pessoa {

	private Integer idade;
	private String cpf;
	
	
	
	//gets and setters
	public Integer getIdade() {
		return idade;
	}
	public void setIdade(Integer idade) {
		this.idade = idade;
	}
	public String getCpf() {
		return cpf;
	}
	public void setCpf(String cpf) {
		this.cpf = cpf;
	}
	
	
}
